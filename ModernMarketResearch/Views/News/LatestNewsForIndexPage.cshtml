@model IEnumerable<ModernMarketResearch.Models.spLatestNews_Result>
@{
    ViewBag.Title = "LatestNewsForIndexPage";
    Layout = null;
}
<div class="row text-center">
    <div class="col-xs-12">
        <h2 class="section-title">Letest News</h2>
    </div>
</div>
<!-- /.row -->
<div class="row content-row">
    <div class="col-md-12">
        <div class="col-md-12 col-xs-12 feature-post">
            <div class="col-md-12">
                @foreach (var item in Model)
                {
                    <div class="col-md-12 col-sm-12">
                        <div class="news-content">
                            <h3>@item.NewsTitle</h3>
                            <p>@Html.Raw(item.LongDescription)</p>
                        </div>
                    </div>
                }
            </div>
        </div>
    </div>
</div>
